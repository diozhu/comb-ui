{
    "presets": [
        ["env", {
            "modules": false,
            "targets": {
                "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
            }
        }],
        "stage-3"
    ],
    "plugins": ["transform-vue-jsx", "transform-runtime"],
    "env": {
        "utils": {
            "plugins": [
                ["module-resolver", {
                    "root": ["comb-ui"],
                    "alias": {
                        "element-ui/src": "comb-ui/dist"
                    }
                }]
            ]
        }
    }
}
